<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:first="clr-namespace:Application.Client.D.Windows.Views.First"
                    xmlns:viewModels="clr-namespace:Application.Client.D.Windows.Views.First.ViewModels"
                    xmlns:second="clr-namespace:Application.Client.D.Windows.Views.Second"
                    xmlns:viewModels1="clr-namespace:Application.Client.D.Windows.Views.Second.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:FirstViewModel}">
        <first:FirstView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:SecondViewModel}">
        <second:SecondView/>
    </DataTemplate>
</ResourceDictionary>