<window:DialogWindow x:Class="Application.Client.Windows.Implementations.MessageDialog.Window.MessageDialogWindow"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     xmlns:window="clr-namespace:Application.Client.Windows.DialogWindow.Window;assembly=Application.Client.Windows.DialogWindow"
                     mc:Ignorable="d"
        ResizeMode="NoResize" SizeToContent="WidthAndHeight"
        MinWidth="200" MinHeight="200"
        Title="{Binding WindowSettings.Title}">
    <Grid>
        <ContentControl Content="{Binding ContentPresenter}"/>
    </Grid>
</window:DialogWindow>
