<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:signInView="clr-namespace:Application.Client.Windows.Implementations.Main.Window.Views.SignIn"
                    xmlns:signInViewModel="clr-namespace:Application.Client.Windows.Implementations.Main.Window.Views.SignIn.ViewModels.SignIn"
                    xmlns:chatView="clr-namespace:Application.Client.Windows.Implementations.Main.Window.Views.Chat"
                    xmlns:chatViewModel="clr-namespace:Application.Client.Windows.Implementations.Main.Window.Views.Chat.ViewModels.Chat">
    <DataTemplate DataType="{x:Type signInViewModel:SignInViewModel}">
        <signInView:SignInView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type chatViewModel:ChatViewModel}">
        <chatView:ChatView/>
    </DataTemplate>
</ResourceDictionary>