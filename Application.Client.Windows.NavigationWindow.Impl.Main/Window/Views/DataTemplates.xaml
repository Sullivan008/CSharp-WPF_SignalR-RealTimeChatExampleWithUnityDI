<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:chat="clr-namespace:Application.Client.Windows.NavigationWindow.Impl.Main.Window.Views.Chat"
                    xmlns:chatViewModel="clr-namespace:Application.Client.Windows.NavigationWindow.Impl.Main.Window.Views.Chat.ViewModels.Chat"
                    xmlns:signIn="clr-namespace:Application.Client.Windows.NavigationWindow.Impl.Main.Window.Views.SignIn"
                    xmlns:signInViewModel="clr-namespace:Application.Client.Windows.NavigationWindow.Impl.Main.Window.Views.SignIn.ViewModels.SignIn">
    <DataTemplate DataType="{x:Type signInViewModel:SignInViewModel}">
        <signIn:SignInView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type chatViewModel:ChatViewModel}">
        <chat:ChatView/>
    </DataTemplate>
</ResourceDictionary>